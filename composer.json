{
    "name": "it-healer/laravel-ethereum",
    "description": "A library for Laravel that allows you to create and manage the Ethereum cryptocurrency.",
    "homepage": "https://github.com/it-healer/laravel-ethereum",
    "license": "MIT",
    "authors": [
        {
            "name": "IT-HEALER",
            "email": "info@it-healer.com"
        }
    ],
    "keywords": [
        "it-healer",
        "laravel",
        "ethereum",
        "php"
    ],
    "type": "library",
    "require": {
        "php": "^8.2",
        "ext-gmp": "*",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.92.0",
        "brick/math": "*",
        "minter/minter-php-bip-44": "^1.2",
        "kornrunner/ethereum-address": "^0.3.0",
        "kornrunner/ethereum-offline-raw-tx": "^0.8.0"
    },
    "autoload": {
        "psr-4": {
            "ItHealer\\LaravelEthereum\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ItHealer\\LaravelEthereum\\EthereumServiceProvider"
            ],
            "aliases": {
                "Ethereum": "ItHealer\\LaravelEthereum\\Facades\\Ethereum"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
